/*
	J. Pollard's "Rho" method, 1975.

	Algorithm 5.2.1 from "Prime Numbers - A Computational Perspective"
	by Richard Crandall and Carl Pomerance, 2nd Ed., 2010.
*/
static A, N;
define F(x) = (x^2 + A) % N;

define rho(N_) {
    /* Returns one factor of N if possible,
       or 0 for invalid N,
       or 1 for prime N. */
    local a, s, U, V g;

    /* Sanity check: N must be >4 and not prime */
    if (guff_ptest(N_))
	return 1;
    if (N_ <= 4)
	return 0;

    N = N_;
    /* Choose seeds */
choose_seeds:
## In Calc, random() is half-closed; upper limits here are 1+ source material.
    a = random(1, N - 2);
    s = random(0, N);
    U = V = s;

    /* Factor search */
factor_search:
    A = a;
    U = F(U);
    V = F(V);
    V = F(V); /* F(V) intentionally invoked twice */
    g = gcd(U - V, N);
    if (g == 1)
	goto factor_search;

    /* Bad seed */
    if (g == N)
	goto choose_seeds;

    /* Success */
    return g;
}

define rhofactor(N) {
    /* Factorizes N exclusively by rho() */
    local f, t0 = runtime();

    if (2 > (N = guff_validate(N)))
	return N;

    while (1 < (f = rho(N))) {
	if (f == N)
	    break;
	if (guff_ptest(f))
	    print f;
	else
	    rhofactor(f);
	N /= f;
    }

    if (N > 1 && N != f)
	print N;
    print "(done in", runtime()-t0, "seconds)";
    return f;
}
